    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "resolutions": {
        "glob-parent": ">=5.1.2",
        "trim": ">=0.0.3",
        "nth-check": ">=2.0.1",
        "trim-newlines": ">=3.0.1"
