{
    "name": "photino-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "preinstall": "npm install --package-lock-only --ignore-scripts --no-audit && npx npm-force-resolutions",
        "start": "craco start",
        "build": "craco build && npm run build:tsc",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint . --ext=js",
        "lint:fix": "eslint . --ext=js --fix",
        "watch:tsc": "tsc --watch -p ./tsconfig.json",
        "build:tsc": "tsc -p ./tsconfig.json",
        "storybook": "start-storybook -p 6925 -s public",
        "build-storybook": "build-storybook -s public",
        "analyze": "webpack-bundle-analyzer build/stats.json"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^6.4.0",
        "react-query": "^3.34.12",
        "react-router-dom": "^6.2.1",
        "react-scripts": "5.0.1",
        "classnames": "^2.3.1",
        "luxon": "^1.27.0",
        "mutationobserver-shim": "^0.3.7",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@craco/craco": "^6.4.3",
        "@fullhuman/postcss-purgecss": "^4.1.3",
        "@storybook/addon-actions": "^6.4.18",
        "@storybook/addon-essentials": "^6.4.18",
        "@storybook/addon-links": "^6.4.18",
        "@storybook/addon-storyshots": "^6.4.18",
        "@storybook/builder-webpack5": "^6.4.18",
        "@storybook/manager-webpack5": "^6.4.18",
        "@storybook/node-logger": "^6.4.18",
        "@storybook/preset-create-react-app": "^4.0.0",
        "@storybook/react": "^6.4.18",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.4.0",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
        "enzyme": "^3.11.0",
        "enzyme-to-json": "^3.6.2",
        "eslint-plugin-jest": "^26.0.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-simple-import-sort": "^5.0.3",
        "husky": "^7.0.4",
        "jest": "^27.5.0",
        "jest-watch-typeahead": "^0.6.5",
        "lint-staged": "^12.3.3",
        "npm-force-resolutions": "0.0.10",
        "postcss": "^8.4.6",
        "purgecss-from-html": "^4.1.1",
        "timemachine": "^0.3.2",
        "typescript": "^4.5.5",
        "webpack": "^5.68.0",
        "webpack-bundle-analyzer": "^4.6.1"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
